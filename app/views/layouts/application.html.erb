<!DOCTYPE html>
<html>
  <head>
    <title>DashboardApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <style> 
      #tere { 
          width:80%; 
          margin-left: 10%;
          margin-right: 10%;
      } 
  </style>
  </head>

  <body>
    <%= render 'layouts/navbar' %>
    <div id="tere">
    <%= render 'layouts/alerts' %>
    <%= yield %>
  </div>
  </body>
</html>
